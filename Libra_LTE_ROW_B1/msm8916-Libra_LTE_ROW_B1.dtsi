		#include "batterydata-nokia-BV_L5C-2400mAh-Libra_LTE_ROW_B1.dtsi"
	};
};

&pm8916_bms {
	status = "ok";
	qcom,battery-data = <0xbd>;
};

&qcom_tzlog {
	status = "okay";
};

&qcom_rng {
	status = "okay";
};

&qcom_crypto {
	status = "okay";
};

&qcom_cedev {
	status = "okay";
};

&qcom_seecom {
	status = "okay";
};

/* CoreSight */
&spmi_bus {
	qcom,pm8916@1 {
		qcom,vibrator@c000 {
			status = "okay";
			qcom,vib-timeout-ms = <15000>;
			qcom,vib-vtg-level-mV = <0xbb8>;
		};
	};
};
