#include "msm8916.dtsi"
#include "msm8916-pinctrl.dtsi"
#include "msm8916-camera-sensor-mtp.dtsi"
#include "dsi_giza_720p_video-Libra_LTE_ROW_B1.dtsi"
#include "dsi_innolux_720p_video-Libra_LTE_ROW_B1.dtsi"

/ {
	aliases {
		serial0 = &blsp1_uart2;
	};
};

&soc {
	i2c@78b9000 { /* BLSP1 QUP5 */
		synaptics_dsx@4B {
			compatible = "synaptics,dsx";
			reg = <0x4b>;
			interrupt-parent = <0x28>;
			interrupts = <13 0x2008>;
			vdd-supply = <0xaa>;
			vcc_i2c-supply = <0x23>;
			synaptics,pwr-reg-name = "vdd";
			synaptics,bus-reg-name = "vcc_i2c";
			synaptics,irq-gpio = <0x28 0xd 0x2008>;
			synaptics,irq-flags = <0x2008>;
			synaptics,power-delay-ms = <0xa0>;
			synaptics,reset-gpio = <0x28 0xc 0x0>;
			synaptics,reset-on-state = <0x0>;
			synaptics,reset-active-ms = <0xa>;
			synaptics,reset-delay-ms = <0x64>;
			synaptics,cap-button-codes = <0x9e 0xac>;
			synaptics,firmware-name = "synaptics/libralte_fw_update";
			synaptics,max-y-for-2d = <0x500>;
			synaptics,vir-button-codes = <0x9e 0xb4 0x578 0x64 0x3c 0xac 0x21c 0x578 0x64 0x3c>;
		};
	};

	gen-vkeys {
		compatible = "qcom,gen-vkeys";
		label = "synaptics_rmi4_i2c";
		qcom,disp-maxx = <1079>;
		qcom,disp-maxy = <1919>;
		qcom,panel-maxx = <1079>;
		qcom,panel-maxy = <2084>;
		qcom,key-codes = <158 139 172 217>;
	};

	qcom,iris-fm {
		compatible = "qcom,iris_fm";
		qcom,fm-lna-supported;
		qcom,fm-lna-gpio = <0x28 0xb 0x0>;
	};

	i2c@78ba000 { /* BLSP1 QUP6 */
		nfc-nci@e {
			compatible = "qcom,nfc-nci";
			reg = <0x0e>;
			qcom,irq-gpio = <0x28 0x15 0x0>;
			qcom,dis-gpio = <0x28 0x14 0x0>;
			qcom,clk-src = "BBCLK2";
			qcom,clk-en-gpio = <0x28 0x0 0x0>;
			interrupt-parent = <0x28>;
			interrupts = <21 0>;
			interrupt-names = "nfc_irq";
			pinctrl-names = "nfc_active","nfc_suspend";
			pinctrl-0 = <0xb2 0xb3>;
			pinctrl-1 = <0xb4 0xb5>;
			qcom,clk-gpio = <0xb6 0x2 0x0>;
			clocks = <0x8 0xfe15cb87>;
			clock-names = "ref_clk";
		};
	};

	i2c@78b6000 { /* BLSP1 QUP2 */
		kionix_acc@1e {
			reg = <0x1e>;
			compatible = "kionix,kionix_acc";
			vdd-supply = <0xaa>;
			vio-supply = <0x23>;
			interrupt-parent = <0x28>;
			interrupts = <0x73 0x2>;
			kionix_acc,min_interval = <0x5>;
			kionix_acc,poll_interval = <0xc8>;
			kionix_acc,accel_direction = <0x2>;
			kionix_acc,accel_irq_use_ddtap = <0x1>;
			kionix_acc,accel_res = <0x3>;
			kionix_acc,accel_g_range = <0x0>;
			kionix_acc,accel_tap_z_direction = <0x1>;
			kionix_acc,accel_dtap_z_direction = <0x1>;
		};

		n2dm_acc@08 {
			reg = <0x8>;
			compatible = "st,n2dm_acc";
			vdd-supply = <0xaa>;
			vio-supply = <0x23>;
			interrupt-parent = <0x28>;
			interrupts = <115 0x2>;
			n2dm_acc,min_interval = <0x5>;
			n2dm_acc,poll_interval = <0xc8>;
			n2dm_acc,accel_direction = <0x2>;
			n2dm_acc,accel_irq_use_ddtap = <0x1>;
			n2dm_acc,accel_g_range = <0x0>;
			n2dm_acc,accel_tap_z_direction = <0x1>;
			n2dm_acc,accel_dtap_z_direction = <0x1>;
		};

		bma250_acc@18 {
			reg = <0x18>;
			compatible = "bma,bma250_acc";
			vdd-supply = <0xaa>;
			vio-supply = <0x23>;
			interrupt-parent = <0x28>;
			interrupts = <0x73 0x2>;
			bma250_acc,min_interval = <0x5>;
			bma250_acc,poll_interval = <0xc8>;
			bma250_acc,accel_direction = <0x2>;
			bma250_acc,accel_g_range = <0x3>;
		};

		apds9930_prox_als@39 {
			reg = <0x39>;
			compatible = "avago,apds9930";
			vdd-supply = <0xaa>;
		        vio-supply = <0x23>;
			pinctrl-names = "default";
			pinctrl-0 = <0xab>;
			interrupt-parent = <0x28>;
			interrupts = <0x71 0x0>;
			apds9930,irq-gpio = <0x28 0x71 0x0>;
			apds9930,ps_detection_threshold = <0x2c4>;
			apds9930,ps_hsythresis_threshold = <0x1ca>;
			apds9930,ps_pulse_number = <0x3>;
			apds9930,ps_pdrive_ma = <0x40>;
			apds9930,ps_pgain = <0x8>;
			apds9930,ps_poffset = <0x0>;
			apds9930,als_threshold_hsytersis = <0x14>;
			apds9930,als_use_irq_in_idle = <0x1>;
			apds9930,als_ga = <0x898>;
			apds9930,als_coe_b = <0xba>;
			apds9930,als_coe_c = <0x52>;
			apds9930,als_coe_d = <0x74>;
			apds9930,als_df = <0x34>;
		};

		betamon_prox@23 {
			reg = <0x23>;
			compatible = "liteon,betamon_prox";
			vdd-supply = <0xaa>;
			vio-supply = <0x23>;
			ltr554,irq-gpio = <0x28 0x71 0x0>;
		};

		akm-mag@0c {
			reg = <0x0c>;
			compatible = "akm,akm09911";
			pinctrl-names = "default", "sleep";
			pinctrl-0 = <0xac>;
			pinctrl-1 = <0xad>;
			akm,layout = <0x8>;
			akm,auto-report;
			vdd-supply = <0xaa>;
			vio-supply = <0x23>;
			akm,gpio_rstn = <0x28 0x45 0x0>;
		};

	};

	sound {
		compatible = "qcom,msm8x16-audio-codec";
		qcom,model = "msm8x16-snd-card";
		qcom,msm-snd-card-id = <0>;
		qcom,msm-codec-type = "internal";
		qcom,msm-ext-pa = "primary";
		qcom,msm-mclk-freq = <9600000>;
		qcom,msm-mbhc-hphl-swh = <0x1>;
		qcom,msm-mbhc-gnd-swh = <0>;
		qcom,msm-hs-micbias-type = "internal";
		qcom,audio-routing =
			"RX_BIAS", "MCLK",
			"SPK_RX_BIAS", "MCLK",
			"INT_LDO_H", "MCLK",
			"MIC BIAS External", "Handset Mic",
			"MIC BIAS Internal2", "Headset Mic",
			"MIC BIAS External", "Secondary Mic",
			"AMIC1", "MIC BIAS External",
			"AMIC2", "MIC BIAS Internal2",
			"AMIC3", "MIC BIAS External";
		pinctrl-names = "cdc_lines_act",
				"cdc_lines_sus",
				"cdc_lines_sec_ext_act",
				"cdc_lines_sec_ext_sus",
				"cross_conn_det_act",
				"cross_conn_det_sus";
		pinctrl-0 = <0xd3>;
		pinctrl-1 = <0xd4>;
		pinctrl-2 = <0xd3 0xd5 0xd6>;
		pinctrl-3 = <0xd4 0xd7 0xd8>;
		pinctrl-4 = <0xd9>;
		pinctrl-5 = <0xda>;
	};
};

&blsp1_uart2 {
	status = "ok";
	pinctrl-names = "default";
	pinctrl-0 = <0x86>;
};

