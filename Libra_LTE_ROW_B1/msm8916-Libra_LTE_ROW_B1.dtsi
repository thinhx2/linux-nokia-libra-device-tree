		};

		bom_d {
			id = "unknown";
			voltage_min = <0xb329f>;
			voltage_max = <0xd1401>;
		};

		bom_e {
			id = "unknown";
			voltage_min = <0x93350>;
			voltage_max = <0xb329e>;
		};

		bom_f {
			id = "unknown";
